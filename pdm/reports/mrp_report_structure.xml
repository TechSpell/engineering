<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <!-- Injected Standard Templates -->
    <template id="report_mrp_bom" name="enhance_report_mrp_bom" inherit_id="mrp.report_mrp_bom">
         <xpath expr="//th[@name='th_mrp_bom_h']" position="after">
             <th>Revision</th>
             <th>Status</th>
             <th>Description</th>
         </xpath>
        <xpath expr="//td[@name='td_mrp_bom']" position="after">
             <td>
                 <span><a href="#"><t t-esc="data['engineering_revision']"/></a></span>
             </td>
             <td>
                 <span><a href="#"><t t-esc="data['state']"/></a></span>
             </td>
             <td>
                 <span><a href="#"><t t-esc="data['description']"/></a></span>
             </td>
        </xpath>
        <xpath expr="//td[@name='td_mrp_bom_f']" position="before">
             <td/>
             <td/>
             <td/>
        </xpath>
    </template>

    <template id="report_mrp_bom_pdf_line" name="enhance_report_mrp_bom_pdf_line" inherit_id="mrp.report_mrp_bom_pdf_line">
         <xpath expr="//td[@name='td_mrp_code']" position="after">
             <td>
                <span> <a><t t-esc="l['engineering_revision']"/></a> </span>
             </td>
             <td>
                <span> <a><t t-esc="l['state']"/></a> </span>
             </td>
             <td>
                <span> <a><t t-esc="l['description']"/></a> </span>
             </td>
         </xpath>
    </template>

</odoo>
    